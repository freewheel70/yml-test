{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": ".",
        "dest": "api",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "~/breadcrumb/toc.yml"
    },    
    "markdownEngineName": "markdig",
    "fileMetadata": {},
    "template": [],
    "dest": "_site"
  }
}
